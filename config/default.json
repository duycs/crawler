{
    "server": {
        "host": "localhost",
        "port": 3000
    },
    "crawlers": [
        {
            "page": "https://dstock.vndirect.com.vn",
            "pageRegex": "https://dstock.vndirect.com.vn/d-rating-chuyen-sau/{1}",
            "codes": [
                "HPG",
                "VNM",
                "AGR",
                "CTS",
                "BSI",
                "HCM",
                "FTS",
                "SSI",
                "VCI",
                "TVS",
                "VDS"
            ],
            "dataTable": "data",
            "schedule": true,
            "timezone": "Asia/Ho_Chi_Minh",
            "scheduleExpressions": "00 02 09 * * *"
        }
    ]
}